{
  "compilerOptions": {
    "pretty": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "lib": ["es6"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "baseUrl": ".",
    "paths": {
      "@frontend/auth/*": ["./node_modules/@frontend/auth/src/*"],
      "@frontend/common/*": ["./node_modules/@frontend/common/src/*"],
      "@frontend/users/*": ["./node_modules/@frontend/users/src/*"],
      "@frontend/profile/*": ["./node_modules/@frontend/profile/src/*"],
      "@frontend/utils/*": ["./node_modules/@frontend/utils/src/*"]
    }
  }
}
